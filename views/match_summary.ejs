<!DOCTYPE html>
<html>

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <link rel="stylesheet"  href="css/styles.css">
    <title><%= title %></title>

    <header class="site-header">
        <nav class="navbar navbar-expand-md navbar-dark bg-steel fixed-top">
            <div class="container">
                <a class="navbar-brand mr-4" href="/">CricPanel</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggle"
                    aria-controls="navbarToggle" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarToggle">
                    <div class="navbar-nav mr-auto">
                        <a class="nav-item nav-link" href="/">Home</a>
                        <a class="nav-item nav-link" href="/new_match">New Match</a>
                        <a class="nav-item nav-link" href="">Start Innings</a>
                        <a class="nav-item nav-link" href="">Live Score</a>
                        <a class="nav-item nav-link" href="">Score Board</a>
                    </div>
                    <!-- Navbar Right Side -->
                    <div class="navbar-nav">
                        <a class="nav-item nav-link" href="">Schedule</a>
                        <a class="nav-item nav-link" href="">Account</a>
                    </div>
                </div>
            </div>
        </nav>
    </header>
</head>
<body>

<main role="main" class="container">
    <div class="row">
        <div class="col-md-8">
            <!-- Match Details and Innings Details -->
            <div class="container">
                <span class="d-block p-2 bg-dark text-white">
                    <%= matchdetails.team1Name %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; v/s &nbsp;&nbsp;&nbsp;&nbsp; <%= matchdetails.team2Name %>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Toss Won : <%= matchdetails.tossWonBy %>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Choose to : <%= matchdetails.tossResult %> First
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                </span>
            </div>  
            <br>
            <div class="container">
                
                <% if(matchresult == 1){ %>
                    <span class="d-block p-2 bg-dark text-white ">
                    Match Result&nbsp;&nbsp;&nbsp; : &nbsp;&nbsp;&nbsp;<%=chasewin.teamWon %> won by <%= chasewin.wicketsleft %> Wickets with <%= chasewin.ballsleft %> Balls Left
                    </span>
                <% }  %>
                <% if(matchresult == 2){ %>
                    <span class="d-block p-2 bg-dark text-white">
                    Match Result&nbsp;&nbsp;&nbsp; :&nbsp;&nbsp;&nbsp; <%= defendwin.teamWon %> won by <%= defendwin.wonbyruns %> Runs
                    </span>
                <% } %>
                <% if(matchresult == 3){ %>
                    <span class="d-block p-2 bg-dark text-white">
                    Match Result :  Draw Match
                    </span>
                <% } %>

            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= batinnings1.batteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Batting ScoreCard 
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Total Score : <%= batinnings1.totalScore %> / <%= batinnings1.totalWickets %>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Overs :  <%= batinnings1.oversPlayed %> / <%= batinnings1.totalOvers %>
                </span>
            </div> 
            <br>
            <div class="container">
                <!-- Team1 Batsman's Score Display-->
               <table class="table table-hover table-borderless table-light my-4">
                   <thead class="thead-dark">
                       <tr>
                           <th scope="col">Batter</th>
                           <th scope="col">R</th>
                           <th scope="col">B</th>
                           <th scope="col">4s</th>
                           <th scope="col">6s</th>
                           <th scope="col">SR</th>
                           <th scope="col">Status</th>
                       </tr>
                   </thead>
                   <tbody>
           
                       <% team1batsmans.forEach(function(batsman){ %>
                        <tr>
                            <th scope="row"><%= batsman.name %></th>
                            <td><%= batsman.runs %></td>
                            <td><%= batsman.balls %></td>
                            <td><%= batsman.fours %></td>
                            <td><%= batsman.sixes %></td>
                            <td><%= batsman.strikeRate %></td>
                            <td><%= batsman.outString %></td>
                        </tr>
                        <% }); %>
                   </tbody>
               </table>
            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= bowlteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Bowling ScoreCard 
                </span>
            </div>
            <br>
            <div class="container">
               <!-- Team1 Bowlers Over Display-->
               <table class="table table-hover my-4">
                   <thead class="thead-dark">
                       <tr>
                           <th scope="col">Bowler</th>
                           <th scope="col">O</th>
                           <th scope="col">M</th>
                           <th scope="col">R</th>
                           <th scope="col">W</th>
                           <th scope="col">NB</th>
                           <th scope="col">WD</th>
                       </tr>
                   </thead>
                   <tbody>
                    <% team1bowlers.forEach(function(bowler){ %>
                        <tr>
                            <th scope="row"><%= bowler.name %></th>
                            <td><%= bowler.overs %>.<%= bowler.balls %></td>
                            <td><%= bowler.maidens %></td>
                            <td><%= bowler.runs %></td>
                            <td><%= bowler.wickets %></td>
                            <td><%= bowler.noballs %></td>
                            <td><%= bowler.wides %></td>
                        </tr>
                    <% }) %>
                   </tbody>
               </table>
            </div> 
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= bowlteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp;All Overs Timeline 
                </span>
            </div>
            <br> 
            <div class="container">
                    <!--Team1 Bowling - All overs -->
                <table class="table table-hover table-borderless table-light my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Over No</th>
                            <th scope="col">Bowler Name</th>
                            <th scope="col">Over Score</th>
                            <th scope="col">Over Timeline</th>
                        </tr>
                    </thead>
                    <tbody>
            
                        <% team1bowlinnings.forEach(function(x){ %>
                            <tr>
                                <th scope="row"><%= x.overNo %></th>
                                <td><%= x.bowlerName %></td>
                                <td><%= x.overScore %></td>
                                <td><%= x.overTimeline %></td>
                            </tr>
                            <% }); %>
                    </tbody>
                </table>
            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= batinnings2.batteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Batting ScoreCard 
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Total Score : <%= batinnings2.totalScore %> / <%= batinnings2.totalWickets %>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    Overs : <%= batinnings2.oversPlayed %> / <%= batinnings2.totalOvers %>
                </span>
            </div> 
            <br>
            <div class="container">
                <!-- Team2 Batsman's Score Display-->
               <table class="table table-hover table-borderless table-light my-4">
                   <thead class="thead-dark">
                       <tr>
                           <th scope="col">Batter</th>
                           <th scope="col">R</th>
                           <th scope="col">B</th>
                           <th scope="col">4s</th>
                           <th scope="col">6s</th>
                           <th scope="col">SR</th>
                           <th scope="col">Status</th>
                       </tr>
                   </thead>
                   <tbody>
           
                       <% team2batsmans.forEach(function(batsman){ %>
                        <tr>
                            <th scope="row"><%= batsman.name %></th>
                            <td><%= batsman.runs %></td>
                            <td><%= batsman.balls %></td>
                            <td><%= batsman.fours %></td>
                            <td><%= batsman.sixes %></td>
                            <td><%= batsman.strikeRate %></td>
                            <td><%= batsman.outString %></td>
                        </tr>
                        <% }); %>
                   </tbody>
               </table>
            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= bowlteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Bowling ScoreCard 
                </span>
            </div>
            <br>
            <div class="container">
                <!-- Team2 Bowlers Over Display-->
                <table class="table table-hover my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Bowler</th>
                            <th scope="col">O</th>
                            <th scope="col">M</th>
                            <th scope="col">R</th>
                            <th scope="col">W</th>
                            <th scope="col">NB</th>
                            <th scope="col">WD</th>
                        </tr>
                    </thead>
                    <tbody>
                     <% team2bowlers.forEach(function(bowler){ %>
                         <tr>
                             <th scope="row"><%= bowler.name %></th>
                             <td><%= bowler.overs %>.<%= bowler.balls %></td>
                             <td><%= bowler.maidens %></td>
                             <td><%= bowler.runs %></td>
                             <td><%= bowler.wickets %></td>
                             <td><%= bowler.noballs %></td>
                             <td><%= bowler.wides %></td>
                         </tr>
                     <% }) %>
                    </tbody>
                </table>
            </div> 
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= bowlteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; All Overs Timeline 
                </span>
            </div>
            <br> 
            <div class="container">
                    <!--Team2 Bowling - All overs -->
                <table class="table table-hover table-borderless table-light my-4">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">Over No</th>
                        <th scope="col">Bowler Name</th>
                        <th scope="col">Over Score</th>
                        <th scope="col">Over Timeline</th>
                    </tr>
                </thead>
                <tbody>
        
                    <% team2bowlinnings.forEach(function(x){ %>
                        <tr>
                            <th scope="row"><%= x.overNo %></th>
                            <td><%= x.bowlerName %></td>
                            <td><%= x.overScore %></td>
                            <td><%= x.overTimeline %></td>
                        </tr>
                        <% }); %>
                </tbody>
                </table>
            </div>
        </div> 
        
        <div class="col-md-4">
            <div class="container">
                <!--Extras Team1 Display-->
                <table class="table table-hover my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col"><%= extras1.bowlteamName %></th>
                            <th scope="col">Extras</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">Wides</th>
                            <td><%= extras1.wides %></td>
                        </tr>
                        <tr>
                            <th scope="row">No Balls</th>
                            <td><%= extras1.noballs %></td>
                        </tr>
                        <tr>
                            <th scope="row">Byes</th>
                            <td><%= extras1.byes %></td>
                        </tr>
                        <tr>
                            <th scope="row">Leg Byes</th>
                            <td><%= extras1.legbyes %></td>
                        </tr>
                        <tr>
                            <th scope="row">Total</th>
                            <td><%= extras1.total %></td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= batteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Fall Of Wickets 
                </span>
            </div>
            <br>
            <div class="container">
                <!-- Fall of Wickets Team1 Display-->
                <table class="table table-hover my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Fall of Wickets</th>
                            <th scope="col">Time of Fall</th>
                            <th scope="col">Team Score</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% wicketsfallen1.forEach(function(fallen){ %>
                        <tr>
                            <th scope="row"><%= fallen.batsmanName %></th>
                            <td><%= fallen.overNo %></td>
                            <td><%= fallen.score %></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div> 
            <div class="container">
                <!--Extras Team2 Display-->
                <table class="table table-hover my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col"><%= extras2.bowlteamName %></th>
                            <th scope="col">Extras</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">Wides</th>
                            <td><%= extras2.wides %></td>
                        </tr>
                        <tr>
                            <th scope="row">No Balls</th>
                            <td><%= extras2.noballs %></td>
                        </tr>
                        <tr>
                            <th scope="row">Byes</th>
                            <td><%= extras2.byes %></td>
                        </tr>
                        <tr>
                            <th scope="row">Leg Byes</th>
                            <td><%= extras2.legbyes %></td>
                        </tr>
                        <tr>
                            <th scope="row">Total</th>
                            <td><%= extras2.total %></td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <br>
            <div class="container">
                <span class="d-block p-2 bg-info text-white">
                    <%= bowlteamName %> &nbsp;&nbsp;&nbsp; - &nbsp;&nbsp;&nbsp; Fall Of Wickets 
                </span>
            </div>
            <br>
            <div class="container">
                <!-- Fall of Wickets Team2 Display-->
                <table class="table table-hover my-4">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Fall of Wickets</th>
                            <th scope="col">Time of Fall</th>
                            <th scope="col">Team Score</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% wicketsfallen2.forEach(function(fallen){ %>
                        <tr>
                            <th scope="row"><%= fallen.batsmanName %></th>
                            <td><%= fallen.overNo %></td>
                            <td><%= fallen.score %></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div> 
        </div>   
    </div>
</main>
